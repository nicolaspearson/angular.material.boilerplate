{
	"cSpell.words": [
		"Customizer",
		"Datepicker",
		"Injectable",
		"OffCanvas",
		"Paginator",
		"Preloader",
		"Quickview",
		"Sidenav",
		"Submenu",
		"Tooltips",
		"Truthy",
		"Validators",
		"angularclass",
		"authed",
		"callouts",
		"classlist",
		"codelyzer",
		"devtools",
		"echarts",
		"ecommerce",
		"hammerjs",
		"heatmap",
		"jasminewd",
		"keycodes",
		"macarons",
		"ngrx",
		"ngtools",
		"nospace",
		"pglayout",
		"polyfills",
		"scrollreveal",
		"selectable",
		"slimscroll",
		"tslib",
		"webfontloader"
	],
	"editor.formatOnPaste": false,
	"editor.formatOnSave": true,
	"editor.autoIndent": true,
	"editor.tabSize": 4,
	"editor.detectIndentation": true,
	"editor.insertSpaces": false,
	"files.autoSave": "off",
	"javascript.validate.enable": false,
	"javascript.format.enable": false,
	"prettier.useTabs": true,
	"prettier.typescriptEnable": [
		"typescript"
	],
	"prettier.singleQuote": true,
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/dist": true
	},
	"tslint.enable": true,
	"tslint.exclude": "**/node_modules/**/*",
	"tslint.validateWithDefaultConfig": true,
	"tslint.autoFixOnSave": false,
	"typescript.format.enable": true,
	"typescript.validate.enable": true,
	"typescript.referencesCodeLens.enabled": true,
	"window.zoomLevel": 0
}
